{
  "version": 2,
  "builds": [
    {
      "src": "free_api/api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "forewarn/api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/free-api/(.*)",
      "dest": "/free_api/api/$1"
    },
    {
      "src": "/forewarn/(.*)",
      "dest": "/forewarn/api/$1"
    },
    {
      "src": "/",
      "dest": "/free_api/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}