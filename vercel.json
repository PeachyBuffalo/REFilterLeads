{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "static",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/free-api/(.*)",
      "dest": "/free_api/api/$1"
    },
    {
      "src": "/forewarn/(.*)",
      "dest": "/forewarn/api/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/free_api/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}